{
  "name": "react-native-vulpes",
  "version": "0.3.4",
  "description": "React Native implementation of the Vulpes design system",
  "main": "src/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-vulpes.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"{src,example/src}/**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "postinstall": "patch-package",
    "bootstrap": "yarn example && yarn && yarn pods",
    "deploy-storybook": "storybook-to-ghpages --remote gh --existing-output-dir=example/storybook-static/",
    "commit": "cz"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "http://bitbucket.org",
  "author": "Leonardo de miranda Borba <borba@gogood.com.br> (https://gogood.com.br)",
  "license": "MIT",
  "bugs": {
    "url": "http://bitbucket.org/issues"
  },
  "homepage": "http://bitbucket.org#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.16",
    "@commitlint/config-conventional": "^15.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@storybook/storybook-deployer": "^2.8.7",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "commitizen": "^4.2.4",
    "commitlint": "^15.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.5",
    "react": "^17.0.0",
    "react-native-builder-bob": "^0.17.1",
    "release-it": "^14.2.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "native-base": "^2.15.2",
    "react": "*",
    "react-native": "*",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-svg": "^12.1.1",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  },
  "storybook-deployer": {
    "gitUsername": "GoGood",
    "gitEmail": "borba@gogood.com.br",
    "commitMessage": "docs: deploying storybook [skip ci]"
  },
  "dependencies": {
    "core-js": "^3.9.0",
    "cz-conventional-changelog": "^3.3.0",
    "d3-shape": "^3.1.0",
    "gradient-parser": "^1.0.0",
    "patch-package": "^6.1.0-0",
    "react-native-dash": "^0.0.11",
    "react-native-dashed-border": "^2.1.1",
    "react-native-device-info": "^8.0.2",
    "react-native-gifted-charts": "^1.0.3",
    "react-native-icomoon": "^0.1.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-qrcode-svg": "^6.1.1",
    "react-native-render-html": "^6.3.1",
    "react-native-sideswipe": "^1.5.0",
    "react-native-svg": "^12.1.1",
    "react-native-svg-charts": "^5.4.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "~0.17.0",
    "react-native-web-linear-gradient": "^1.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
